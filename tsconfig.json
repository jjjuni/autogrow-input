{
    "compilerOptions": {
        "target": "ES2019",
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "outDir": "dist"
    },
    "include": [
        "src"
    ]
}